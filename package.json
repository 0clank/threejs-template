{
  "name": "threejs-template",
  "version": "1.0.0",
  "description": "Three.js project preset",
  "main": "index.js",
  "scripts": {
    "run:build": "webpack --config webpack.dev.config.js --mode=development",
    "run:watch": "webpack --config webpack.dev.config.js --mode=development --watch",
    "run:serve": "webpack serve --config webpack.dev.config.js --mode=development",
    "run:production": "webpack --config webpack.prod.config.js --mode=production",
    "test:serve": "karma start karma.conf.js",
    "test:headless": "karma start --browsers ChromeHeadless karma.conf.js",
    "test:ci": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "lint:sass": "sass-lint -v -q",
    "lint:ts": "eslint . --ext .ts",
    "lint:ts:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NinetiesScrapper/threejs-template.git"
  },
  "keywords": [
    "three.js"
  ],
  "author": "Danny Becker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NinetiesScrapper/threejs-template/issues"
  },
  "homepage": "https://github.com/NinetiesScrapper/threejs-template#readme",
  "devDependencies": {
    "@types/jasmine": "^3.7.7",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "html-webpack-plugin": "^5.3.1",
    "jasmine": "^3.7.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-tap": "^4.2.0",
    "karma-webpack": "^5.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "three": "^0.129.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
