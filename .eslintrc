{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
      "@typescript-eslint"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-use-before-define": 0,
      "func-names": 0,
      "prefer-arrow-callback": 0,
      "no-var": 1,
      "guard-for-in": 0,
      "object-shorthand": 0,
      "no-restricted-syntax": 0,
      "prefer-template": 0,
  }
}